generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  nickname  String    @unique
  phone     String    @unique
  picture   String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model Item {
  id         Int    @id @default(autoincrement())
  name       String
  price      Int
  uploaderId Int
}
